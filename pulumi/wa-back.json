[
    {
        "name": "back",
        "image": "thecodingmachine/workadventure-back:v1.15.12",
        "essential": true,
        "portMappings": [
            {
                "name": "back",
                "containerPort": 8080,
                "hostPort": 8080
            }
        ],
        "dockerLabels": {
            "traefik.http.routers.back.rule": "Host(`api.workadventure.aws.ocho.ninja`)",
            "traefik.enable": "true"
        },
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "/ecs/WorkAdventure",
                "awslogs-region": "eu-west-3",
                "awslogs-stream-prefix": "back"
            }
        },
        "environment": [
            {
                "name": "PLAY_URL",
                "value": "https://play.workaventure.aws.ocho.ninja"
            },
            {
                "name": "SECRET_KEY",
                "value": "ricirici"
            },
            {
                "name": "JITSI_URL",
                "value": "meet.jit.si"
            },
            {
                "name": "BBB_URL",
                "value": "https://test-install.blindsidenetworks.com/bigbluebutton/"
            },
            {
                "name": "BBB_SECRET",
                "value": "8cd8ef52e8e101574e400365b55e11a6"
            },
            {
                "name": "MAX_PER_GROUP",
                "value": "4"
            },
            {
                "name": "STORE_VARIABLES_FOR_LOCAL_MAPS",
                "value": "true"
            },
            {
                "name": "REDIS_HOST",
                "value": "redis.workadventure"
            },
            {
                "name": "MAP_STORAGE_URL",
                "value": "map-storage:50053"
            },
            {
                "name": "PUBLIC_MAP_STORAGE_URL",
                "value": "https://map-storage.workadventure.aws.ocho.ninja"
            },
            {
                "name": "PLAYER_VARIABLES_MAX_TTL",
                "value": "-1"
            },
            {
                "name": "EJABBERD_API_URI",
                "value": "https://ejabberd.workadventure.aws.ocho.ninja/api"
            },
            {
                "name": "EJABBERD_DOMAIN",
                "value": "ejabberd.workadventure.aws.ocho.ninja"
            },
            {
                "name": "EJABBERD_USER",
                "value": "admin"
            },
            {
                "name": "EJABBERD_PASSWORD",
                "value": "mySecretPassword"
            },
            {
                "name": "ENABLE_CHAT",
                "value": "true"
            },
            {
                "name": "ENABLE_CHAT_UPLOAD",
                "value": "true"
            }
        ]
    }
]